const handleEditExam = () => {
  if (currentExamId !== null) { // Ensure currentExamId is not null
    setData(data.map(exam => (exam.id === String(currentExamId) ? { ...exam, ...newExam } : exam)));
  }
  resetForm();
};



interface Exam {
  id: string; // Ensure id is a string
  examName: string;
  examDate: string;
  startTime: string;
  endTime: string;
  key: number; // If key is necessary
}

// Assuming currentExamId is declared somewhere
const [currentExamId, setCurrentExamId] = useState<string | null>(null);
