const handleAddExam = (e: React.FormEvent) => {
    e.preventDefault();
    const newExam = {
        id: data.length + 1, // or a more unique ID
        examName,
        examDate: examDate.format("DD-MM-YYYY"),
        startTime: startTimeValue,
        endTime: endTimeValue,
        key: data.length + 1, // Add this line to include the key
    };
    setData([...data, newExam]);
    setExamName("");
    setExamDate(defaultValue);
    setStartTimeValue("");
    setEndTimeValue("");
};
